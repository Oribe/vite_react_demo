{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "node"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "component/*": [
        "src/component/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "style/*": [
        "src/style/*"
      ],
      "view/*": [
        "src/view/*"
      ],
      "route/*": [
        "src/route/*"
      ],
      "store/*": [
        "src/store/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "layout/*": [
        "src/layout/*"
      ],
      "/~": [
        "src/*"
      ],
    }
  },
  "include": [
    "src"
, "react-app-env.d.ts"  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}